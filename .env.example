# Expo public environment variables
# Rename this file to `.env` or `.env.local` and fill in real values before running the app.
# Never commit files containing production credentials.

# Supabase configuration (required for media uploads)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=supabase-anon-key
EXPO_PUBLIC_SUPABASE_BUCKET=UploadEvidence

# Optional service integrations
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=google-maps-key
EXPO_PUBLIC_TWILIO_ACCOUNT_SID=twilio-account-sid
EXPO_PUBLIC_TWILIO_AUTH_TOKEN=twilio-auth-token
EXPO_PUBLIC_TWILIO_PHONE_NUMBER=+10000000000
EXPO_PUBLIC_AI_SERVICE_API_KEY=ai-service-key
AI_SERVICE_URL=https://ai-service.example.com
AI_SERVICE_API_KEY=ai-service-secret
